	"endpoints": {
		"users": {
			"get": {
				"description": "return all registered users",
				"request": {},
				"response": {
					"users": [
						{
							"email": "string",
							"id": "string",
							"username": "string"
						}
					]
				}
			},
			"post": {
				"description": "Register a new user",
				"request": {
					"username": "string",
					"email": "string",
					"password": "string",
					"verify_password": "string"
				},
				"response": {
					"token": "string",
					"user": {
						"username": "string",
						"id": "string",
						"email": "string"
					}
				},
				"error": {
					"error": "string"
				}
			}
		},
		"user": {
			"get /:id":{
				"description": "returns a user, like if there's an existing token in the browser",
				"request": {},
				"response": {
					"user": {
						"email": "string",
						"id": "string",
						"username": "string"
					}
				},
				"error": {
					"error": "string"
				}
			},
			"post": {
				"description": "login user",
				"request": {
					"email": "string",
					"password": "password"
				},
				"response": {
					"user": {
						"email": "string",
						"id": "string",
						"username": "string"
					}
				},
				"error": {
					"error": "string"
				}
			},
			"put /:id": {
				"description": "change username, password, email",
				"request": {
					"username": "string",
					"email": "string",
					"current_password": "string",
					"password": "string",
					"verify_password": "string"
				},
				"response": {
					"user": {
						"email": "string",
						"id": "string",
						"username": "string"
					}
				},
				"error": {
					"error": "string"
				}
			},
			"delete /:id": {
				"description": "delete user instance",
				"request": {},
				"response": {
					"id": "string"
				},
